@using System.Web.UI.WebControls
@using Stimulsoft.Report
@using Stimulsoft.Report.Mvc
@{
    ViewBag.Title = "ReportShow";
    string loc = "~/Content/Localization/en.xml";
}


<div>
    @Html.Stimulsoft().RenderMvcViewerFxScripts()

    @Html.Stimulsoft().StiMvcViewerFx(new StiMvcViewerFxOptions
{
    Theme = StiMvcViewerFxOptions.Themes.Black,
    ActionGetReportSnapshot = (string)ViewBag.NameAction,
    ActionExportReport = "ExportReport",
    PrintAllowToHtml = true,
    PrintAllowToPdf = true,
    Width = Unit.Percentage(100),
    Height = 600
})
</div>





